[core]
	autocrlf = input
	safecrlf = true
	editor = vim

[alias]
        l = log --stat
	hist = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
        stat = shortlog -sn

	co = checkout
	st = status

[color]
	ui = true
	diff = auto
	status = auto
	branch = auto
